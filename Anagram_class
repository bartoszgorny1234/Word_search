class Anagram:
    def __init__(self, x: int, y: int):
        self._x = x
        self._y = y
        self._board=[]

    def create_board(self):
        emptylist = []
        for _ in range(0, self._x):
            emptylist.append("")
        for _ in range(0, self._y):
            self._board.append(emptylist)

    def show_board(self):
        for row in self._board:
            print(row)



test = Anagram(4,5)

test.create_board()
test.show_board()